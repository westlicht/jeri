{
  "name": "jeri",
  "version": "1.1.0",
  "license": "Modified Apache 2.0 License (see the license file)",
  "homepage": "https://jeri.io",
  "keywords": [
    "EXR",
    "OpenEXR",
    "HDR",
    "image",
    "viewer"
  ],
  "description": "JavaScript Extended-Range Image viewer",
  "dependencies": {
    "@types/react": ">=16.0.0",
    "@types/react-dom": ">=16.0.0",
    "@types/styled-components": "^4.1.8",
    "common-prefix": "1.1.0",
    "normalize-wheel": "1.0.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "file-loader": "^3.0.1",
    "json-loader": "0.5.7",
    "serve": "^10.1.2",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "worker-loader": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run build-npm && npm run build-web",
    "build-npm": "tsc && cp -r ./src/exr-wrap build_npm && cp -r ./src/utils/exr-parser.worker.js build_npm/utils",
    "build-web": "webpack",
    "clean": "rm -rf build build_web/jeri* build_web/*.worker.js build_web/*.wasm build_web/*.worker.js.map",
    "docs": "typedoc --out documentation src",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "examples": "serve build_web"
  },
  "main": "build_npm/jeri.js",
  "types": "build_npm/jeri.d.ts"
}
